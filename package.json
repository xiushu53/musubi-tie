{
  "name": "reach-you",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && prisma db push --accept-data-loss && tsx scripts/migrate-facilities.ts && tsx scripts/seed-dummy-inquiries.ts && node scripts/generateGeohashData.mjs && next build",
    "generate-geohash": "node scripts/generateGeohashData.mjs",
    "generate-geohash:single": "node scripts/generateGeohashData.mjs",
    "clean-geohash": "rimraf public/data",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "dummy:add": "tsx scripts/add-dummy-data.ts",
    "dummy:clear": "tsx scripts/add-dummy-data.ts clear",
    "dummy:reset": "pnpm dummy:clear && pnpm dummy:add",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@deck.gl/core": "^9.1.14",
    "@deck.gl/geo-layers": "^9.1.14",
    "@deck.gl/layers": "^9.1.14",
    "@deck.gl/react": "^9.1.14",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "deck.gl": "^9.1.14",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.539.0",
    "maplibre-gl": "^5.6.2",
    "next": "15.4.6",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-leaflet": "^5.0.0",
    "react-map-gl": "^8.0.4",
    "resend": "^6.0.1",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@tailwindcss/postcss": "^4",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.20",
    "@types/maplibre-gl": "^1.14.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
